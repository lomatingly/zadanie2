#include <iostream>
using namespace std;
int del(int x){
    int i,S;
    S=0;
    for (i=1;i<=(x/2);i++) {
        if (x%i==0){
            S=S+i;
        }
    }
    return S;
}

int main() {
    int i;
    int a1[10000];
    for (i=1;i<=10000;i++){
        if (del(i)<=10000){
            a1[i]=del(i);
        }
    }
    for (i=1;i<=10000;i++){
        if ((a1[a1[i]]==i) and (i!=a1[i]) and (i<a1[i])){
            cout<<i<<' '<<a1[i]<<"\n";
        }
    }
    return 0;
}
