#include <iostream>
#include <stdlib.h>
#include <windows.h>
#include <time.h>
using namespace std;

void random(){
    srand(time(NULL));
    SetConsoleOutputCP(CP_UTF8);
    int a,b;
    a=rand();
    b=rand();
    cout<<"Число на первом игральном кубике-"<<a<<"\n";
    cout<<"Число на первом игральном кубике-"<<b;
}
int main() {
    random();
}
